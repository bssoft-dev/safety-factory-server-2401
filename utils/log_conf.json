{
    "version": 1,
    "disable_existing_loggers": "no",
    "formatters": {
      "simple": {
        "format": "%(asctime)s[%(levelname)s] %(message)s"
      }
    },
    "handlers": {
      "uvicorn": {
        "class": "logging.handlers.TimedRotatingFileHandler",
        "level": "INFO",
        "formatter": "simple",
        "when": "midnight",
        "interval": 1,
        "backupCount": 0,
        "filename": "./logs/uvicorn.log"
      }
    },
    "loggers": {
      "uvicorn": {
        "level": "INFO",
        "handlers": [
          "uvicorn"
        ],
        "propagate": "yes",
        "qualname": "uvicorn"
      }
    }
  }
